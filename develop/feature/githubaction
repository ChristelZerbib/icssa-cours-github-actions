name: github-actions
on: 
  push : 
    branches : 
      -develop
jobs:
  git-hub-action :
    name : Running Units Tests
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v1
      - name: Use Node.js ${{matrix.node-version}}
        uses: actions/setup-node@v1
        with:
          node-version:${{matrix.node-version}}
      - name
      - run: npm install 
      - run: npm test
